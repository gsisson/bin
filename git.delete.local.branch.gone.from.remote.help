#!/usr/bin/env bash

cat << EOF
# The commands needed after, say, merging a PR onto master
# up on origin and having the branch deleting on origin:
#
# 1) remove local knowledge of that remote branch
     git remote prune origin --dry-run
     git remote prune origin
# 2) remove local branch
     git branch -d BRANCH_GONE_FROM_REMOTE
EOF
